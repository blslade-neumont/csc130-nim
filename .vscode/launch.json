{
    "version": "0.2.0",
    "configurations": [{
        "name": "Python",
        "type": "python",
        "pythonPath":"${config:python.pythonPath}",
        "request": "launch",
        "stopOnEntry": false,
        "console": "integratedTerminal",
        "program": "${workspaceRoot}/src/main.py",
        "cwd": "${workspaceRoot}",
        "debugOptions": [
            "WaitOnAbnormalExit",
            "WaitOnNormalExit",
            "RedirectOutput"
        ],
        "env": {"name":"value"}
    }],
    "compounds": []
}
